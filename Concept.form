|-
|}

= norska =
<table class="formtable">
<tr><th>{{int:ttp-form-concept-help}}</th><td>{{int:ttp-form-concept-help-text}}</td></tr>
<tr><th>{{int:ttp-form-concept-definition}}</th><td>{{{field|määritelmä_sv|input type=textarea}}}</td></tr>
<tr><th>{{int:ttp-form-concept-explanation}}</th><td>{{{field|selite_sv|input type=textarea}}}</td></tr>
<tr><th>{{int:ttp-form-concept-moreinfo}}</th><td>{{{field|lisätiedot_sv|input type = textarea}}}</td></tr>
</table><br>

= English =
<table class="formtable">
<tr><th>{{int:ttp-form-concept-help}}</th><td>{{int:ttp-form-concept-help-text}}</td></tr>
<tr><th>{{int:ttp-form-concept-definition}}</th><td>{{{field|määritelmä_en|input type=textarea}}}</td></tr>
<tr><th>{{int:ttp-form-concept-explanation}}</th><td>{{{field|selite_en|input type=textarea}}}</td></tr>
<tr><th>{{int:ttp-form-concept-moreinfo}}</th><td>{{{field|lisätiedot_en|input type = textarea}}}</td></tr>
</table>

<headertabs />

{| class="formtable"
! {{int:ttp-form-concept-images}}      || {{{field|kuvat|input type=text|values from namespace=Tiedosto|list}}} {{int:ttp-form-concept-upload}}
