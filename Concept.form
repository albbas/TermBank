<table class="formtable">
<tr><th>{{int:ttp-form-concept-help}}</th><td>{{int:ttp-form-concept-help-text}}</td></tr>
<tr><th>{{int:ttp-form-concept-definition}}</th><td>{{{field|definition_en|input type=textarea}}}</td></tr>
